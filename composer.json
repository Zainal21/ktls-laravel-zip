{
  "name": "zainal21/ktls-laravel-zip",
  "description": "ZipArchive toolbox",
  "license": "MIT",
  "keywords": [
    "zip",
    "unzip",
    "ZipArchive",
    "multiple",
    "merge"
  ],
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\Zip\\ZipServiceProvider"
      ],
      "aliases": {
        "Zip": "ZanySoft\\Zip\\ZipFacade"
      }
    }
  },
  "homepage": "http://www.zanysoft.co",
  "authors": [
    {
      "name": "Zainal21",
      "email": "info@kataliskreasi.com",
      "homepage": "http://kataliskreasi.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ZanySoft\\Zip\\": "src"
    }
  },
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0|^5.0",
    "scrutinizer/ocular": "^1.0"
  }
}
